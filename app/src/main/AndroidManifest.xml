<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.marquant.inc.busticket">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ktravelersicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.marquant.inc.busticket.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.marquant.inc.busticket.Register" />
        <activity
            android:name="com.marquant.inc.busticket.Dashboard"
            android:label="@string/title_activity_dashboard"
            android:parentActivityName="com.marquant.inc.busticket.Login"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.marquant.inc.busticket.ForgotPassword"
            android:parentActivityName="com.marquant.inc.busticket.Login" />
        <activity
            android:name="com.marquant.inc.busticket.BookTicket"
            android:parentActivityName="com.marquant.inc.busticket.Dashboard" />
        <activity
            android:name="com.marquant.inc.busticket.BookingStatus"
            android:parentActivityName="com.marquant.inc.busticket.Dashboard" />
        <activity
            android:name="com.marquant.inc.busticket.AboutUs"
            android:parentActivityName="com.marquant.inc.busticket.Dashboard" />
        <activity
            android:name="com.marquant.inc.busticket.Reschedule"
            android:parentActivityName="com.marquant.inc.busticket.Dashboard" />
        <activity
            android:name="com.marquant.inc.busticket.CancelBooking"
            android:parentActivityName="com.marquant.inc.busticket.Dashboard" />
        <activity
            android:name="com.marquant.inc.busticket.UserProfile"
            android:parentActivityName="com.marquant.inc.busticket.Dashboard" />
        <activity android:name="com.marquant.inc.busticket.PayPal" />
    </application>

</manifest>